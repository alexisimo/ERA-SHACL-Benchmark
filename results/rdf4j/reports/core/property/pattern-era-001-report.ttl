[] a sh:ValidationReport;
  sh:conforms false;
  rdf4j:truncated false;
  sh:result [ a sh:ValidationResult;
      sh:focusNode <http://data.europa.eu/949/functionalInfrastructure/sidings/41f97688067c71186b284ba9c7b85c1e259167df>;
      rsx:shapesGraph rdf4j:SHACLShapeGraph;
      sh:value "HU55";
      sh:resultPath <http://data.europa.eu/949/imCode>;
      sh:sourceConstraintComponent sh:PatternConstraintComponent;
      sh:resultSeverity sh:Violation;
      sh:sourceShape <http://data.europa.eu/949/shapes/imCodeS>
    ] .

<http://data.europa.eu/949/shapes/imCodeS> a sh:PropertyShape;
  sh:path <http://data.europa.eu/949/imCode>;
  sh:pattern "^[0-9]{4}$" .
