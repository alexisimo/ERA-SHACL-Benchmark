[] a sh:ValidationReport;
  sh:conforms false;
  rdf4j:truncated false;
  sh:result [ a sh:ValidationResult;
      sh:focusNode <http://data.europa.eu/949/functionalInfrastructure/operationalPoints/8fb9a95852ddf67349c2a794d3e2059ba3004808>;
      rsx:shapesGraph rdf4j:SHACLShapeGraph;
      sh:resultPath <http://data.europa.eu/949/opName>;
      sh:sourceConstraintComponent sh:MinCountConstraintComponent;
      sh:resultSeverity sh:Violation;
      sh:sourceShape <http://data.europa.eu/949/shapes/opName>
    ] .

<http://data.europa.eu/949/shapes/opName> a sh:PropertyShape;
  sh:path <http://data.europa.eu/949/opName>;
  sh:minCount 1 .
