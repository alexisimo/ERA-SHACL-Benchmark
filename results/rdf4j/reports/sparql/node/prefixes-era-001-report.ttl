@prefix dash: <http://datashapes.org/dash#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rsx: <http://rdf4j.org/shacl-extensions#> .
@prefix rdf4j: <http://rdf4j.org/schema/rdf4j#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

[] a sh:ValidationReport;
  sh:conforms false;
  rdf4j:truncated false;
  sh:result [ a sh:ValidationResult;
      sh:focusNode <http://data.europa.eu/949/functionalInfrastructure/tracks/36bf1461f295d432dd780fdc507133f908b7a1d4>;
      rsx:shapesGraph rdf4j:SHACLShapeGraph;
      sh:sourceConstraint <http://data.europa.eu/949/shapes/ProtectionLegacySystemSKOS>;
      sh:sourceConstraintComponent sh:SPARQLConstraintComponent;
      sh:resultSeverity sh:Violation;
      sh:sourceShape <http://data.europa.eu/949/shapes/trackShape>
    ] .

<http://data.europa.eu/949/shapes/trackShape> a sh:NodeShape;
  sh:targetNode <http://data.europa.eu/949/functionalInfrastructure/tracks/07946c41d18b51d16a9fa7013dc13d30e3374c39>,
    <http://data.europa.eu/949/functionalInfrastructure/tracks/36bf1461f295d432dd780fdc507133f908b7a1d4>;
  sh:sparql <http://data.europa.eu/949/shapes/ProtectionLegacySystemSKOS> .

<http://data.europa.eu/949/shapes/ProtectionLegacySystemSKOS> a sh:SPARQLConstraint;
  sh:select """
    SELECT $this  ?concept
    WHERE {
        $this era:protectionLegacySystem ?concept .
        era:protectionLegacySystem era:inSkosConceptScheme ?conceptScheme .
		FILTER NOT EXISTS{         
  			?concept skos:inScheme ?conceptScheme .
		}
	}
""";
  sh:prefixes <http://data.europa.eu/949/shapes/EraPrefixesDeclaration> .

<http://data.europa.eu/949/shapes/EraPrefixesDeclaration> sh:declare [
      sh:prefix "era";
      sh:namespace "http://data.europa.eu/949/"^^xsd:anyURI
    ], [
      sh:prefix "skos";
      sh:namespace "http://www.w3.org/2004/02/skos/core#"^^xsd:anyURI
    ], [
      sh:prefix "rdfs";
      sh:namespace "http://www.w3.org/2000/01/rdf-schema#"^^xsd:anyURI
    ] .
